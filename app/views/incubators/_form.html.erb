<div class='col-xs-12 col-sm-8 col-sm-2 col-md-6 col-md-offset-3'>
  <%= simple_form_for(@incubator) do |f| %>
    <%= f.error_notification %>
    <div class="form-inputs">
      <%= f.input :name, :required => true, placeholder: "Name of Incubator", label: false%>
    </div>
    <div class="form-actions">
      <%= f.button :submit, class: "btn btn-blue" %>
      <%= link_to 'Back', incubators_path, class: "btn btn-blue" %>
    </div>
  <% end %>
</div>

