<header class="navbar navbar-static-top navbar-static-top-main">
  <div class = "collapse navbar-collapse">
    <ul class = "nav navbar-nav">
      <li><img src="http://res.cloudinary.com/small-change/image/upload/v1461426069/tree-24-xxl_jmymdr.png" class="navbar-icon"></li>
      <li><a href = "#"><i class="fa fa-search" aria-hidden="true"></i> browse startups</a></li>
      <li><a href = "#"><i class="fa fa-area-chart" aria-hidden="true"></i> dashboard</a></li>
      <li><a href = "#"><i class="fa fa-info" aria-hidden="true"></i> about startup tree</a></li>
      <li>
    </ul>
    <ul class="nav navbar-nav navbar-right push-back">
      <li>
        <% if current_page?('/users/sign_in') %>
          <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name), :html => {:class => 'form-inline' }) do |f| %>
            <%= f.error_notification %>
              <div class="form-inputs form-group">
                <%= f.input_field :email, required: false, autofocus: true, label: false, placeholder: 'Email', class: 'form-control input-lg main-input devise-input' %>
                <%= f.input_field :password, required: false, label: false, placeholder: 'Password', class: 'form-control input-lg main-input devise-input' %>
              </div>
              <div class="form-actions">
                <%= f.button :submit, "Log in", class: 'btn btn-log-in btn-sm' %>
              </div>
          <% end %>
        <% end %>
        <% if current_user %>
          <li><%= link_to ('<i class="fa fa-sign-in"></i> Sign out').html_safe, destroy_user_session_path, method: :delete %></li>
        <% end %>
      </li>
    </ul>
  </div>
</header>






        



